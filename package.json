{
  "name": "jslib",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "test": "turbo run test",
    "cs": "changeset",
    "publish-packages": "turbo run build test && changeset version && changeset publish",
    "lint-staged": "lint-staged --config lint-staged.lint.js",
    "lint": "run-s \"lint:*\"",
    "lint:prettier": "prettier \"**/*.{js,mjs,cjs,ts,json}\" \"./src/styles/**/*.css\" --check --plugin-search-dir=. --ignore-path=.gitignore --no-error-on-unmatched-pattern",
    "lint:eslint": "eslint \"**/*.{js,mjs,cjs,ts}\" --no-error-on-unmatched-pattern",
    "lint:stylelint": "stylelint \"./src/styles/**/*.css\" --allow-empty-input",
    "fix-staged": "lint-staged --config lint-staged.fix.js",
    "fix": "run-s \"fix:*\"",
    "fix:prettier": "prettier \"**/*.{js,mjs,cjs,ts,json}\" \"./src/styles/**/*.css\" --write --plugin-search-dir=. --ignore-path=.gitignore --no-error-on-unmatched-pattern",
    "fix:eslint": "eslint \"**/*.{js,mjs,cjs,ts}\" --fix --no-error-on-unmatched-pattern",
    "fix:stylelint": "stylelint \"./src/styles/**/*.css\" --fix --allow-empty-input"
  },
  "dependencies": {
    "@babel/core": "^7.22.5",
    "@babel/eslint-parser": "^7.22.5",
    "@changesets/cli": "^2.26.1",
    "@types/node": "^20.3.1",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "@vitest/ui": "^0.32.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-turbo": "^1.10.3",
    "eslint-import-resolver-node": "^0.3.7",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-unicorn": "^47.0.0",
    "eslint-plugin-vitest-globals": "^1.3.1",
    "lint-staged": "^13.2.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "stylelint": "^15.7.0",
    "stylelint-config-standard": "^33.0.0",
    "tsc-files": "^1.1.3",
    "tsup": "^6.7.0",
    "turbo": "^1.10.3",
    "typescript": "^5.1.3",
    "vite": "^4.3.9",
    "vite-tsconfig-paths": "^4.2.0",
    "vitest": "^0.32.0"
  }
}
