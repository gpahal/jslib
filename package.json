{
  "name": "jslib",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "test": "turbo run test",
    "cs": "changeset",
    "publish-packages": "turbo run build test && changeset version && changeset publish",
    "lint": "run-s \"lint:*\"",
    "lint:eslint": "eslint \"**/*.{js,mjs,cjs,ts}\"",
    "lint:stylelint": "stylelint \"**/*.css\"",
    "lint-fix": "run-s \"lint-fix:*\"",
    "lint-fix:eslint": "eslint \"**/*.{js,mjs,cjs,ts}\" --fix",
    "lint-fix:stylelint": "stylelint \"**/*.css\" --fix",
    "lint-staged": "lint-staged --config lint-staged.lint.cjs",
    "format": "prettier \"**/*.{js,mjs,cjs,ts,json,css}\" --check --ignore-path=.gitignore",
    "format-fix": "prettier \"**/*.{js,mjs,cjs,ts,json,css}\" --write --ignore-path=.gitignore",
    "format-staged": "lint-staged --config lint-staged.format.cjs"
  },
  "dependencies": {
    "@babel/core": "^7.22.5",
    "@babel/eslint-parser": "^7.22.5",
    "@changesets/cli": "^2.26.1",
    "@types/node": "^20.3.1",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@typescript-eslint/parser": "^5.60.0",
    "@vitest/ui": "^0.32.2",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-turbo": "^1.10.6",
    "eslint-import-resolver-node": "^0.3.7",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-unicorn": "^47.0.0",
    "eslint-plugin-vitest-globals": "^1.3.1",
    "lint-staged": "^13.2.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "stylelint": "^15.9.0",
    "stylelint-config-standard": "^33.0.0",
    "tsc-files": "^1.1.3",
    "tsup": "^7.0.0",
    "turbo": "^1.10.6",
    "typescript": "^5.1.3",
    "vite": "^4.3.9",
    "vite-tsconfig-paths": "^4.2.0",
    "vitest": "^0.32.2"
  }
}
