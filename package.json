{
  "name": "jslib",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "test": "turbo run test",
    "cs": "changeset",
    "publish-packages": "turbo run build test && changeset version && changeset publish",
    "lint": "run-s \"lint:*\"",
    "lint:eslint": "eslint \"**/*.{js,mjs,cjs,ts,jsx,tsx}\" --ignore-path=.gitignore",
    "lint:stylelint": "stylelint \"**/*.css\" --ignore-path=.gitignore",
    "lint-fix": "run-s \"lint-fix:*\"",
    "lint-fix:eslint": "eslint \"**/*.{js,mjs,cjs,ts,jsx,tsx}\" --fix --ignore-path=.gitignore",
    "lint-fix:stylelint": "stylelint \"**/*.css\" --fix --ignore-path=.gitignore",
    "lint-staged": "lint-staged --config lint-staged.lint.cjs",
    "fmt": "prettier \"**/*.{js,mjs,cjs,ts,jsx,tsx,css,json}\" --check --ignore-path=.gitignore",
    "fmt-fix": "prettier \"**/*.{js,mjs,cjs,ts,jsx,tsx,css,json}\" --write --ignore-path=.gitignore",
    "fmt-staged": "lint-staged --config lint-staged.fmt.cjs"
  },
  "dependencies": {
    "@babel/core": "^7.22.9",
    "@babel/eslint-parser": "^7.22.9",
    "@changesets/cli": "^2.26.2",
    "@ianvs/prettier-plugin-sort-imports": "^4.0.2",
    "@types/node": "^20.4.2",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitest/ui": "^0.33.0",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-turbo": "^1.10.7",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-unicorn": "^47.0.0",
    "eslint-plugin-vitest-globals": "^1.4.0",
    "lint-staged": "^13.2.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.0",
    "stylelint": "^15.10.1",
    "stylelint-config-standard": "^34.0.0",
    "tsc-files": "^1.1.4",
    "tsup": "^7.1.0",
    "turbo": "^1.10.7",
    "typescript": "^5.1.6",
    "vite": "^4.4.3",
    "vite-tsconfig-paths": "^4.2.0",
    "vitest": "^0.33.0"
  }
}
