{
  "name": "@gpahal/eslint-config",
  "version": "0.0.93",
  "author": "Garvit Pahal <g10pahal@gmail.com>",
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    "./*": {
      "import": "./build/*.js",
      "types": "./build/*.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "build/*.d.ts"
      ]
    }
  },
  "files": [
    "package.json",
    "README.md",
    "src",
    "build"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "clean": "premove build .turbo *.tsbuildinfo"
  },
  "dependencies": {
    "@eslint/js": "^9.30.0",
    "@eslint/json": "^0.12.0",
    "@gpahal/std": "workspace:*",
    "@typescript-eslint/parser": "^8.35.1",
    "eslint-config-flat-gitignore": "^2.1.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-depend": "^1.2.0",
    "eslint-plugin-import-x": "^4.16.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-mdx": "^3.5.0",
    "eslint-plugin-n": "^17.20.0",
    "eslint-plugin-regexp": "^2.9.0",
    "eslint-plugin-unicorn": "^59.0.1",
    "globals": "^16.2.0",
    "typescript-eslint": "^8.35.1"
  },
  "devDependencies": {
    "@eslint-react/eslint-plugin": "^1.52.2",
    "@next/eslint-plugin-next": "^15.3.4",
    "@typescript-eslint/utils": "^8.35.1",
    "@vitest/eslint-plugin": "^1.3.3",
    "eslint-plugin-astro": "^1.3.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-turbo": "^2.5.4"
  },
  "peerDependencies": {
    "@eslint-react/eslint-plugin": "^1.52",
    "@next/eslint-plugin-next": "^15.3",
    "@vitest/eslint-plugin": "^1.3",
    "eslint-plugin-astro": "^1.3",
    "eslint-plugin-react-hooks": "^5.2",
    "eslint-plugin-turbo": "^2.5"
  },
  "peerDependenciesMeta": {
    "@eslint-react/eslint-plugin": {
      "optional": true
    },
    "@next/eslint-plugin-next": {
      "optional": true
    },
    "@vitest/eslint-plugin": {
      "optional": true
    },
    "eslint-plugin-astro": {
      "optional": true
    },
    "eslint-plugin-react-hooks": {
      "optional": true
    },
    "eslint-plugin-turbo": {
      "optional": true
    }
  }
}
