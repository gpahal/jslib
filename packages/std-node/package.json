{
  "name": "@gpahal/std-node",
  "version": "0.0.19",
  "author": "Garvit Pahal <g10pahal@gmail.com>",
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    "./*": {
      "require": "./build/*.cjs",
      "import": "./build/*.js",
      "types": "./build/*.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "build/*.d.ts"
      ]
    }
  },
  "files": [
    "package.json",
    "README.md",
    "build"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "prepublishOnly": "pnpm build"
  },
  "dependencies": {
    "@gpahal/std": "workspace:*"
  }
}
