{
  "name": "@gpahal/std-node",
  "version": "0.0.8",
  "author": "Garvit Pahal <g10pahal@gmail.com>",
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    "./*": {
      "require": "./dist/*.cjs",
      "import": "./dist/*.js",
      "types": "./dist/*.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/*.d.ts"
      ]
    }
  },
  "files": [
    "README.md",
    "package.json",
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "prepublishOnly": "pnpm build"
  },
  "dependencies": {
    "@gpahal/std": "workspace:*"
  }
}
