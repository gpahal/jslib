{
  "name": "@gpahal/font-data-inter",
  "version": "0.0.9",
  "author": "Garvit Pahal <g10pahal@gmail.com>",
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    ".": "./index.css",
    "./font-info": {
      "require": "./build/font-info.cjs",
      "import": "./build/font-info.js",
      "types": "./build/font-info.d.ts"
    },
    "./files/*": "./files/*"
  },
  "typesVersions": {
    "*": {
      "font-info": [
        "build/font-info.d.ts"
      ]
    }
  },
  "main": "index.css",
  "files": [
    "package.json",
    "README.md",
    "build",
    "index.css",
    "files"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "prepublishOnly": "pnpm build"
  },
  "devDependencies": {
    "@gpahal/font": "workspace:*"
  }
}
