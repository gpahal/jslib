{
  "name": "@gpahal/std",
  "version": "0.0.21",
  "author": "Garvit Pahal <g10pahal@gmail.com>",
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    "./*": {
      "require": "./build/*.cjs",
      "import": "./build/*.js",
      "types": "./build/*.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "build/*.d.ts"
      ]
    }
  },
  "files": [
    "package.json",
    "README.md",
    "src",
    "build"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "prepublishOnly": "pnpm build",
    "test": "vitest run"
  },
  "dependencies": {
    "ts-custom-error": "^3.3.1"
  }
}
